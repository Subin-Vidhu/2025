{
  "manifest_version": 3,
  "name": "Export Tabs URLs",
  "version": "1.0",
  "description": "Copy all open tabs' URLs to clipboard or download as a file.",
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "tab_export.png",
    "32": "tab_export.png",
    "48": "tab_export.png",
    "128": "tab_export.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Export Tabs",
    "default_icon": {
      "16": "tab_export.png",
      "32": "tab_export.png",
      "48": "tab_export.png",
      "128": "tab_export.png"
    }
  }
}